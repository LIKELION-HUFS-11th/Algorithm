   "source": [
    "N = int(input())\n",
    "cnt = len(str(N))\n",
    "string = str(N)\n",
    "\n",
    "front, back = 0, 0\n",
    "for i in range(0, int(cnt/2)):\n",
    "    front += int(string[i])\n",
    "    back += int(string[cnt-i-1])\n",
    "\n",
    "if front == back:\n",
    "    print(\"LUCKY\")\n",
    "else: print(\"READY\")"
   ]
